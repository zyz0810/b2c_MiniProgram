@import '/template/authorize/authorize.wxss';
page {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  overflow: hidden;
  background: #f9f9f9;
}

.container {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  overflow: hidden;
}

.header {
  display: flex;
  flex-direction: row;
  height: 30px;
  text-align: center;
  align-items: center;
  justify-content: center;
  background: #fff;
}

.header text {
  padding: 0 20rpx;
  font-weight: bold;
  position: relative;
}

.header .iconfont {
  font-size: 20px;
}

.header text.active {
  color: #ff707a;
}

.header text.active::after {
  content: '';
  display: block;
  position: absolute;
  left: 10%;
  bottom: -10rpx;
  height: 6rpx;
  width: 80%;
  background: #ff707a;
}

.header image {
  display: block;
  width: 30rpx;
  height: 30rpx;
  margin: auto 20rpx;
}

.select {
  padding: 4px 0;
  height: 24px;
  line-height: 25px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  background: #fff;
}

.select .active {
  color: #ff707a;
}

.select view {
  padding: 0 20rpx;
}

.content>view:last-child {
  padding-bottom: 30rpx;
}

scroll-view {
  position: absolute;
  top: 60px;
  width: 100%;
  /* background: #f2f2f2; */
  background: #f6f6f6;
}

.content>view {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
  padding: 0 1.3%;
}

.goodsItem {
  width: 230rpx;
  margin: 1.3% 0.9%;
  box-sizing: border-box;
  padding: 10rpx;
  background: #fff;
  position: relative;
}

.goodsItem image {
  width: 210rpx;
  height: 210rpx;
  display: block;
  margin: 0 auto;
}

.goodsItem .overtwo {
  width: 206rpx;
  line-height: 34rpx;
  /* padding: 10rpx 0 0; */
  margin: 10rpx 0;
  height: 65rpx;
  word-break: break-all;
}

.goodsItem .op {
  display: flex;
  flex-direction: row;
  height: 60rpx;
  line-height: 60rpx;
  justify-content: space-between;
}

.goodsItem .goodsItemNum {
  position: absolute;
  top: -10rpx;
  right: -10rpx;
  font-size: 24rpx;
  background: #ff707a;
  color: #fff;
  min-width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 40rpx;
}

.calc {
  width: 120rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}

.calc input {
  width: 50rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  text-align: center;
}

.calc text {
  display: block;
  width: 50rpx;
  /* height: 60rpx; *//* line-height: 60rpx; */
  font-size: 28rpx;
  text-align: center;
}

.calc image {
  margin: 0;
  display: block;
  width: 35rpx;
  height: 35rpx;
}

.bottom {
  position: absolute;
  z-index: 1001;
  bottom: 0;
  height: 50px;
  width: 100%;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.bottom-num {
  position: absolute;
  z-index: 10002;
  top: -5px;
  right: -5px;
  background: #ff707a;
  color: #fff;
  font-size: 12px;
  height: 24px;
  min-width: 24px;
  line-height: 24px;
  border-radius: 12px;
  padding: 0;
  text-align: center;
}

.bottom-icon {
  position: relative;
  width: 60px;
  height: 60px;
  line-height: 60px;
  box-sizing: border-box;
  border-radius: 50%;
  background: #fff;
  margin-left: 10%;
  margin-top: -18px;
  box-shadow: -2px 0 5px #e4e4e4, 0 -2px 5px #e4e4e4, 0 2px 5px #e4e4e4, 2px 0 5px #e4e4e4;
}

/* .bottom-icon-shadow {
  display: none;
  position: absolute;
  width: 65px;
  height: 65px;
  box-sizing: border-box;
  border-radius: 50%;
  overflow: hidden;
  background: #fff;
  left: 75rpx;
  bottom: 0;
  z-index: 10000;
  box-shadow: -2px 0 5px #e4e4e4, 0 -2px 5px #e4e4e4, 0 2px 5px #e4e4e4, 2px 0 5px #e4e4e4;
} */

.bottom-icon image {
  display: block;
  width: 30px;
  height: 30px;
  margin: 15px;
}

.bottom-price {
  padding-left: 20px;
  margin-right: auto;
}

.bottom-btn {
  color: #fff;
  line-height: 50px;
  padding: 0 20px;
  /* font-weight: bold; */
  background: #ff707a;
  transition: background 0.1s;
}

.actionsheetWrap {
  position: absolute;
  width: 100%;
  height: 100%;
  /* bottom: -100%; */
  bottom: 50px;
}

.actionsheet {
  position: absolute;
  z-index: 10000;
  width: 100%;
  left: 0;
  bottom: 0;
  background: #fff;
  /* box-shadow: 2px 2px 3px #aaa; */
}

.actionsheet-content {
  max-height: 245px;
  overflow: hidden;
  overflow-y: scroll;
}

.actionsheet-head {
  background: #e4e4e4;
  display: flex;
  justify-content: space-between;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 20rpx;
}

.actionsheet-head image {
  width: 30rpx;
  height: 30rpx;
  margin: 15rpx 10rpx 15rpx auto;
}

.acGoodsItem {
  display: flex;
  height: 60px;
  line-height: 60px;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 10px;
  border-bottom: 1px solid #eee;
}

.acGoodsItem .price {
  margin-left: auto;
}

.acGoodsItem-title {
  flex: 1;
}

.acGoodsItem .calc {
  width: 90px;
}

.acGoodsItem input {
  width: 40px;
}

.acGoodsItem image {
  width: 20px;
  height: 20px;
}

.mask {
  position: absolute;
  width: 100%;
  height: 100%;
  /* top: 100%; */
  top: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 1000;
}

.dialog {
  position: absolute;
  width: 100%;
  height: 100%;
  /* left: 100%; */
  left: 0;
  top: 0;
  z-index: 10001;
}

.dialog-content {
  position: absolute;
  width: 80%;
  left: 10%;
  top: 15%;
  box-sizing: border-box;
  padding: 20rpx 60rpx;
  background: #fff;
  border-radius: 20rpx;
  z-index: 1001;
  overflow: hidden;
}

.dialog-content swiper {
  display: block;
  width: 100%;
  height: 605rpx;
}

.dialog-content swiper image {
  display: block;
  width: 480rpx;
  height: 480rpx;
  margin: 20rpx auto 0;
}

.dialog-content swiper-item .price {
  box-sizing: border-box;
  padding: 10rpx 20rpx;
}

.dialog-content .swiperControlLeft, .dialog-content .swiperControlRight {
  position: absolute;
  width: 30rpx;
  height: 60rpx;
  top: 40%;
}

.dialog-content .swiperControlLeft {
  left: 14rpx;
}

.dialog-content .swiperControlRight {
  right: 14rpx;
}

.dialog-op {
  height: 50rpx;
  display: flex;
  border-radius: 15rpx;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 20rpx;
}

.dialog-opTitle {
  width: 240rpx;
  word-break: break-all;
  color: #ffa73d;
}

/*商品  */

.productItem {
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  position: relative;
  margin-bottom: 10rpx;
}

.productItem .productItemImgs {
  width: 100%;
}

.productItem .productItemImgs .imgone image {
  display: block;
  width: 100%;
  height: 750rpx;
}

.productItem .productItemImgs .imgtwo, .productItem .productItemImgs .imgthree {
  display: flex;
  justify-content: space-between;
}

.productItem .productItemImgs .imgtwo image {
  width: 49.5%;
  height: 371.25;
}

.productItem .productItemImgs .imgthree>image {
  width: 66.3%;
  height: 497.25rpx;
}

.productItem .productItemImgs .imgthree>view {
  width: 32.7%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.productItem .productItemImgs .imgthree>view image {
  width: 100%;
}

.productItem .productDetails {
  padding: 2%;
}

.productItem .price {
  line-height: 2;
  margin-bottom: 5px;
}

.productItem .price text {
  margin-right: 20rpx;
}

.productItem .cartIcon image {
  width: 32rpx;
  height: 32rpx;
}

.productItem .cartIcon {
  width: 50rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #ffa73d;
  border-radius: 50%;
  position: absolute;
  bottom: 30rpx;
  right: 50rpx;
}

.productItem .favoriteIcon image {
  width: 32rpx;
  height: 32rpx;
}

.productItem .favoriteIcon {
  width: 50rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  /* border: 1px solid #ffa73d; */
  border-radius: 50%;
  position: absolute;
  bottom: 30rpx;
  right: 130rpx;
}

.coupon {
  background: url('https://cdn.laiyijia.com/upload/image/201906/72708504-7fb4-452f-b69c-1afa90e39724.png') no-repeat;
  background-size: 100% 100%;
  width: 72%;
  height: 60%;
  position: fixed;
  top: 15%;
  left: 14%;
  z-index: 1001;
}
.toMiniProgram {
  background: #fff;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1001;
  padding-top: 50px;
  border-radius: 20rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.toMiniProgram image{
  width: 246rpx;
  height: 274rpx;
}
.tit{
  margin: 50rpx 0 30rpx;
}
.cont{
  line-height: 1.8;
  margin-bottom: 30px;
}
.toMiniProgramBtn{
  background: #0099FF;
  border: none;
  letter-spacing: 1px;
  width: 80%;
}
.toMiniProgramBtn::before,.toMiniProgramBtn::after{
   border: none;
}

.coupon .cancel {
  color: #fff;
  position: absolute;
  top: -10vw;
  right: -8vw;
}

.coupon .coupon_tit {
  width: 50%;
  margin: 55rpx 25%;
}

.coupon_cont {
  width: 80%;
  height: 60%;
  position: absolute;
  bottom: 75rpx;
  left: 10%;
  overflow-y: scroll;
}

.coupon_item {
  width: 100%;
  height: 15vw;
  background: #f2f2f2;
  border-radius: 10rpx;
  /* overflow: hidden; */
  margin-bottom: 20rpx;
}

.coupon_left {
  color: #ff703a;
  width: 20vw;
  text-align: center;
  line-height: 15vw;
  overflow: hidden;
}

.coupon_item .coupon_middle {
  width: 3vw;
  height: 100%;
  position: relative;
  z-index: 999;
}

.coupon_item .coupon_middle .circle {
  width: 3vw;
  height: 3vw;
  border-radius: 50%;
  display: block;
  background: #de1f37;
  position: absolute;
  z-index: 9999;
}

.coupon_item .coupon_middle .circle_top {
  top: -2vw;
}

.coupon_item .coupon_middle .circle_bottom {
  bottom: -2vw;
}

.coupon_item .coupon_middle .line {
  width: 2px;
  height: 60%;
  border-left: 2px dotted #fe7941;
  position: absolute;
  left: 1.2vw;
  top: 20%;
  z-index: 9999;
}

.coupon_item .coupon_right {
  color: #ff703a;
  padding: 22rpx 0 22rpx 10rpx;
  line-height: 1.5em;
}

.coupon_item .coupon_revice {
  color: #fff;
  background: #fe7941;
  height: 100%;
  padding: 0 4%;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.coupon_item .coupon_revice view:nth-child(1) {
  margin-top: 20rpx;
}

.coupon_item.coupon_gray .coupon_revice {
  background: #ccc;
}

.coupon_item.coupon_gray .coupon_left, .coupon_item.coupon_gray .coupon_right {
  color: #ccc;
}

.coupon_item.coupon_gray .coupon_middle .line {
  border-left: 2px dotted #ccc;
}

.coupon .myCoupon {
  position: absolute;
  bottom: 20rpx;
  left: 0;
  text-align: center;
  width: 100%;
  color: #fff;
  text-decoration: #fff;
  text-decoration-line: underline;
}

.timeLimit {
  padding: 40rpx 4%;
  background: #fff;
}

.timeLimit_tag {
  color: #ffe3e3;
  background: #ff707a;
  display: inline-block;
  font-style: italic;
  padding: 2px 10px;
  border-top-left-radius: 16px;
  border-bottom-right-radius: 16px;
  margin-left: 10px;
}

.timeLimit .timeLimit_list {
  padding-top: 4%;
}

.timeLimit .timeLimit_list .lineItem {
  -webkit-box-direction: normal;
  -webkit-box-orient: horizontal;
  -webkit-flex-direction: row;
  -moz-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: stretch;
  -webkit-align-items: stretch;
  -moz-align-items: stretch;
  align-items: stretch;
  margin-bottom: 60rpx;
  padding: 0;
}

.timeLimit .timeLimit_list .lineItem:last-child {
  margin-bottom: 0;
}

.timeLimit .timeLimit_list .lineItem .center {
  padding: 1% 0 1% 2%;
  -webkit-box-direction: normal;
  -webkit-box-orient: vertical;
  -webkit-flex-direction: column;
  -moz-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -moz-justify-content: space-between;
  justify-content: space-between;
}

.timeLimit .timeLimit_list .lineItem .left {
  width: 27vw;
  height: 27vw;
}

.timeLimit .timeLimit_list .lineItem .left image {
  border-radius: 8px;
}

.timeLimit_list .lineItem .center .tag {
  line-height: 1;
}

.timeLimit_list .lineItem .center .tag text {
  color: #ff9c9c;
  background: #ffeded;
  border-radius: 3px;
  line-height: 1.5;
  padding: 0 5px;
  margin-right: 5px;
}

/* .timeLimit_list .lineItem .center .price text{
  margin-right: 20px;
} */

.timeLimit_list .lineItem .center .price {
  position: relative;
}

.old_price {
  color: #999;
  text-decoration: line-through;
  margin-left: 20rpx;
}

.buy {
  padding: 2px 8px;
  border-radius: 30rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  margin-right: 0;
}

.time {
  position: absolute;
  bottom: 4em;
  right: 10rpx;
}

.time text {
  color: #999;
  background: #e6e6e6;
  width: 18px;
  height: 18px;
  line-height: 18px;
  border-radius: 50%;
  display: inline-block;
  margin: 0 2px;
}

/* //规格弹窗 */

.actionMask {
  /* position: absolute; */
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 1001;
  box-sizing: border-box;
}

.actionContent {
  /* position: absolute; */
  position: fixed;
  width: 100%;
  background: #fff;
  left: 0;
  bottom: 0;
  z-index: 1002;
}

.actionTitle {
  padding: 0 20rpx;
}

view {
  overflow: visible;
}

.actionTitle .left image {
  width: 160rpx;
  height: 160rpx;
  position: relative;
  top: -40rpx;
}

.actionTitle .center {
  padding-left: 20rpx;
}

.actionTitle .right image {
  width: 50rpx;
  height: 50rpx;
}

.actionSpecifications {
  padding: 2% 20rpx;
  max-height: 50vh;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}

.actionItemBtn {
  padding: 20rpx 0;
}

.actionItemBtn text {
  display: inline-block;
  padding: 10rpx 20rpx;
  margin: 0 20rpx 20rpx 0;
  white-space: nowrap;
  background: #efeff4;
  color: #fff;
  border-radius: 12rpx;
}

.actionItemBtn text.active {
  color: #fff !important;
}

.actionItemBtn text.canTap {
  color: #000;
}

.actionNumber {
  padding: 2% 20rpx;
}

.actionNumber .amount {
  display: flex;
  width: 210rpx;
  position: relative;
}

.actionNumber .amount::after {
  content: " ";
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: 1px solid rgba(0, 0, 0, 0.2);
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  border-radius: 10px;
}

.actionNumber .amount input {
  display: block;
  position: relative;
}

.actionNumber .amount input::after {
  content: " ";
  width: 200%;
  height: 200%;
  overflow-x: scroll;
  position: absolute;
  top: 0;
  left: 0;
  border-left: 1px solid rgba(0, 0, 0, 0.2);
  border-right: 1px solid rgba(0, 0, 0, 0.2);
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
}

.actionNumber .amount button, .amount input {
  width: 70rpx;
  height: 70rpx !important;
  min-height: 70rpx !important;
  padding: 0;
  margin: 0;
  line-height: 70rpx !important;
  background: #fff;
  text-align: center;
  background: transparent;
  z-index: 999;
}

.actionNumber .amount button::after {
  border: none !important;
}

.defaultBtn {
  margin: 20rpx auto 60rpx;
}
